programa i
função inicio ) 1 //declaração de variáveis
inteiro numero, centena, dezena, unidade, invertido //entrada de dados
escreva("digite um número de três dígitos")
leia (número)
//processamento
centena=numero/100
dezena-(número %100)/100
unidade-número %10
invertido=unidade*100+dezena*10+centena
//disse de dados
escreva("o número invertido e", invertido)
