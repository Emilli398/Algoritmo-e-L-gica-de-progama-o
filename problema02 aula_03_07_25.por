programa {
  funcao inicio() { 
    //declaracao de variaveis 
    cadeia Nome_
    inteiro Idade1,Dias_
    //Entrada de dados 
    escreva("Qual é o seu nome?")
    leia(Nome_)
    escreva("Informe a sua idade:")
    leia(Idade1)
    //Procesamento 
    Dias_=365*Idade1 
    //Saída de dados 
    escreva("Oii ", Nome_, " você já viveu ", Dias_, " dias!","\n")

  
  }
}
