programa {
  funcao inicio() {
      // Declaracao de variaveis 
    real N1,N2,N3,N4,MF 
    //Entrada de dados 
    escreva("Informe a primeira nota")
    leia(N1) 
    escreva("Informe a segunda nota")
    leia(N2)
    escreva("Informe a terceira nota") 
    leia(N3) 
    escreva("Informe a quarta nota") 
    leia(N4) 
    //Processamento 
    MF=(N1+N2+N3+N4)/4
    //Saida dados 
    escreva("A media do aluno e", MF )
  }
}
